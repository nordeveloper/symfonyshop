{{ form_start(form) }}
<div class="form-group">
    {{ form_widget(form.active) }}
 </div>

<div class="form-group">     
    {{ form_widget(form.title, { 'attr': {'class': 'form-control'}}) }}
</div>

<div class="form-group">
    {{ form_widget(form.previewImage) }}
</div>

 <div class="form-group">
    {{ form_widget(form.previewText, { 'attr': {'class': 'form-control'}}) }}
</div>

<div class="form-group">
    {{ form_widget(form.detailImage) }}
</div>    

<div class="form-group">    
    {{ form_widget(form.detailText, { 'attr': {'class': 'form-control'}}) }}
</div>


<div class="form-group">
    <button class="btn btn-success">{{ button_label|default('Save') }}</button>
</div>

{{ include('dashboard/category/_delete_form.html.twig') }}
{{ form_end(form) }}
